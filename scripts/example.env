# CUSTOM2_TEMPLATE="COSMOS"
# CUSTOM2_CHAIN_ID="akashnet-2"
# CUSTOM2_NAME="Akash"
# CUSTOM2_SYMBOL="AKT"
# CUSTOM2_DENOM="uakt"
# CUSTOM2_CHANNEL="channel-43"
