# Example environment variables for IBC assets configuration
# Copy this file to .env.local and update the values as needed

# =======================================
# Predefined Chains
# =======================================

# Intento Chain (uses custom template with minimal required fields)
INTO_CHAIN_ID="intento-ics-test-1"
NEXT_PUBLIC_INTO_CHAIN_ID=${INTO_CHAIN_ID}
INTO_NAME="Intento Testnet"
INTO_SYMBOL="INTO"
INTO_DENOM="uinto"
INTO_LOGO_URI="https://intento.zone/assets/images/icon.png"
INTO_PREFIX="into"
INTO_REGISTRY_NAME="intentodevnet"
NEXT_PUBLIC_INTO_REGISTRY_NAME=${INTO_REGISTRY_NAME}


# Cosmos Hub (using IBC template)
COSMOS_TEMPLATE="IBC"
COSMOS_CHAIN_ID="cosmoshub-4"
COSMOS_NAME="Cosmos Hub"
COSMOS_SYMBOL="ATOM"
COSMOS_DENOM="uatom"
COSMOS_CHANNEL="channel-1"
COSMOS_CHANNEL_TO_INTENTO="channel-1"
COSMOS_RPC="https://rpc-cosmoshub.keplr.app"
COSMOS_LOGO_URI="https://raw.githubusercontent.com/cosmos/chain-registry/master/cosmoshub/images/atom.svg"
COSMOS_PREFIX="cosmos"

# Osmosis (using IBC template)
OSMOSIS_TEMPLATE="IBC"
OSMOSIS_CHAIN_ID="osmosis-1"
OSMOSIS_NAME="Osmosis"
OSMOSIS_SYMBOL="OSMO"
OSMOSIS_DENOM="uosmo"
OSMOSIS_CHANNEL="channel-141"
OSMOSIS_CHANNEL_TO_INTENTO="channel-141"
OSMOSIS_RPC="https://rpc-osmosis.keplr.app"
OSMOSIS_LOGO_URI="https://raw.githubusercontent.com/cosmos/chain-registry/master/osmosis/images/osmo.svg"
OSMOSIS_PREFIX="osmo"

# =======================================
# Custom Chains
# =======================================
# Uncomment and customize the following examples as needed

