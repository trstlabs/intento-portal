# CUSTOM1_TEMPLATE="COSMOS"
# CUSTOM1_CHAIN_ID="juno-1"
# CUSTOM1_NAME="Juno"
# CUSTOM1_SYMBOL="JUNO"
# CUSTOM1_DENOM="ujuno"
# CUSTOM1_CHANNEL="channel-42"
# CUSTOM1_CHANNEL_TO_INTENTO="channel-42"
# CUSTOM1_RPC="https://rpc-juno.itastakers.com"
# CUSTOM1_LOGO_URI="https://raw.githubusercontent.com/cosmos/chain-registry/master/juno/images/juno.png"

