{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/SpotOrder",
    "definitions": {
        "SpotOrder": {
            "properties": {
                "order_type": {
                    "enum": [
                        "STOPLOSS",
                        "LIMITSELL",
                        "LIMITBUY",
                        "MARKETBUY"
                    ],
                    "type": "string",
                    "title": "Spot Order Type",
                    "description": "Spot order"
                },
                "order_id": {
                    "type": "string"
                },
                "legacy_order_price_v1": {
                    "$ref": "#/definitions/elys.tradeshield.LegacyOrderPriceV1",
                    "additionalProperties": false
                },
                "order_price": {
                    "type": "string"
                },
                "order_amount": {
                    "$ref": "#/definitions/cosmos.base.v1beta1.Coin",
                    "additionalProperties": false
                },
                "owner_address": {
                    "type": "string"
                },
                "order_target_denom": {
                    "type": "string"
                },
                "status": {
                    "enum": [
                        "PENDING",
                        "EXECUTED",
                        "CANCELED",
                        "ALL"
                    ],
                    "type": "string",
                    "title": "Status"
                },
                "date": {
                    "$ref": "#/definitions/elys.tradeshield.Date",
                    "additionalProperties": false
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Spot Order"
        },
        "cosmos.base.v1beta1.Coin": {
            "properties": {
                "denom": {
                    "type": "string"
                },
                "amount": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Coin",
            "description": "Coin defines a token with a denomination and an amount. NOTE: The amount field is an Int which implements the custom method signatures required by gogoproto."
        },
        "elys.tradeshield.Date": {
            "properties": {
                "height": {
                    "type": "string"
                },
                "timestamp": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Date"
        },
        "elys.tradeshield.LegacyOrderPriceV1": {
            "properties": {
                "base_denom": {
                    "type": "string"
                },
                "quote_denom": {
                    "type": "string"
                },
                "rate": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Legacy Order Price V 1"
        }
