{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/GenesisState",
    "definitions": {
        "GenesisState": {
            "properties": {
                "minter": {
                    "$ref": "#/definitions/intento.mint.v1.Minter",
                    "additionalProperties": false,
                    "description": "minter is a space for holding current inflation information."
                },
                "params": {
                    "$ref": "#/definitions/intento.mint.v1.Params",
                    "additionalProperties": false,
                    "description": "params defines all the paramaters of the module."
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Genesis State",
            "description": "GenesisState defines the mint module's genesis state."
        },
        "intento.mint.v1.Minter": {
            "properties": {
                "annual_provisions": {
                    "type": "string",
                    "description": "current annual expected provisions"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Minter",
            "description": "Minter represents the minting state."
        },
        "intento.mint.v1.Params": {
            "properties": {
                "mint_denom": {
                    "type": "string",
                    "description": "type of coin to mint"
                },
                "start_time": {
                    "type": "string",
                    "description": "the time the chain starts",
                    "format": "date-time"
                },
                "initial_annual_provisions": {
                    "type": "string",
                    "description": "initial annual provisions"
                },
                "reduction_factor": {
                    "type": "string",
                    "description": "factor to reduce inflation by each year"
                },
                "blocks_per_year": {
                    "type": "string",
                    "description": "expected blocks per year"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Params",
            "description": "Params holds parameters for the mint module."
        }
    }
}